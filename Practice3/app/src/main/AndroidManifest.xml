<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practice3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".A_SimpleAdapter"
            android:label="@string/simpleAdapter"
            />
        <activity
        android:name=".A_AlertDialog"
        android:label="@string/alertdialog"
        />
        <activity android:name=".A_XML"
            android:label="@string/xml"
            />
        <activity
            android:name=".A_ActionMode"
            android:label="@string/actionmode"
            />



    </application>

</manifest>